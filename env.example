# Server Configuration
PORT=3000

# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require
DATABASE_SSL_ENABLED=1

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Blockchain RPC Endpoints
# Configure RPC endpoints for supported networks
BERACHAIN_RPC_ENDPOINT=https://your-berachain-rpc-endpoint
SONIC_RPC_ENDPOINT=https://your-sonic-rpc-endpoint

# Active Deployments
# Comma-separated list of blockchain types to harvest. Leave empty to harvest all deployments
ACTIVE_DEPLOYMENTS=berachain,sonic

# External API Keys
# Required if deployment to Ethereum network is supported
COINGECKO_API_KEY=your-coingecko-api-key
# Required
COINMARKETCAP_API_KEY=your-coinmarketcap-api-key
# Required for non-Ethereum networks
CODEX_API_KEY=your-codex-api-key
# Optional: For additional data querying
DUNE_API_KEY=your-dune-api-key

# Feature Flags
SHOULD_HARVEST=1
SHOULD_UPDATE_ANALYTICS=1
SHOULD_POLL_QUOTES=1
SHOULD_POLL_HISTORIC_QUOTES=1
SEND_NOTIFICATIONS=0
SHOULD_PROCESS_REFERRALS=0

# Polling Intervals (in milliseconds)
POLL_QUOTES_INTERVAL=30000
POLL_HISTORIC_QUOTES_INTERVAL=30000
PROCESS_REFERRALS_INTERVAL=10000
PROCESS_REFERRALS_ON_STARTUP=0

# System Configuration
CARBON_LOCK_DURATION=30
IS_FORK=0
DB_SYNC=0

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/credentials.json
QUEUE_NAME=your-queue-name
QUEUE_LOCATION=your-queue-location
API_URL=your-api-url

# Telegram Configuration
TELEGRAM_CHAT_ID=your-chat-id

# Network-specific Telegram Configuration
ETHEREUM_TELEGRAM_BOT_TOKEN=your-bot-token
ETHEREUM_CARBON_THREAD_ID=your-carbon-thread-id
ETHEREUM_FASTLANE_THREAD_ID=your-fastlane-thread-id
ETHEREUM_VORTEX_THREAD_ID=your-vortex-thread-id
ETHEREUM_EXPLORER_URL=your-explorer-url
ETHEREUM_CARBON_WALLET_URL=your-wallet-url

# External Database Configuration (used by seed script)
EXTERNAL_DATABASE_USERNAME=your-username
EXTERNAL_DATABASE_PASSWORD=your-password
EXTERNAL_DATABASE_HOST=your-host
EXTERNAL_DATABASE_NAME=your-database-name

